@model float

@{
    if (Model < 2)
    {
        <font color="green">@Model %</font>
    }
    else if (Model <= 5)
    {
        <font color="yellow">@Model %</font>
    }
    else if (Model <= 8)
    {
        <font color="orange">@Model %</font>
    }
    else
    {
        <font color="red">@Model %</font>
    }
}